script FortreeCity_Gym_EventScript_Lycanroc{
    lock
    playmoncry(SPECIES_LYCANROC, CRY_MODE_ENCOUNTER)
    delay(24)
    release
    end
}

script FortreeCity_Gym_EventScript_Rockruff{
    lock
    playmoncry(SPECIES_ROCKRUFF, CRY_MODE_ENCOUNTER)
    delay(24)
    release
    end
}

script FortreeCity_Gym_EventScript_GiveStoneEdge{
    giveitem(ITEM_TM_STONE_EDGE)
	msgbox(FortreeCity_Gym_Text_ExplainStoneEdge, MSGBOX_DEFAULT)
	return
}

