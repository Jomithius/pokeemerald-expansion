script NewMauville_Inside_EventScript_Togedemaru{
    lockall
    setflag(FLAG_TOGEDEMARU_TOTEM)
    applymovement(9, Common_Movement_FacePlayer)
    waitmovement(9)
    playmoncry(SPECIES_TOGEDEMARU, CRY_MODE_ENCOUNTER)
    delay(40)
    waitmoncry
    setwildbattle(SPECIES_TOGEDEMARU_TOTEM, 20, ITEM_ROCKY_HELMET)
    dowildbattle
    specialvar(VAR_RESULT, GetBattleOutcome)
    if(var(VAR_RESULT) == B_OUTCOME_CAUGHT){
        call(TogedemaruWon_Caught)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_WON){
        call(TogedemaruWon_Caught)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_RAN){
		call(TogedemaruFailed)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_PLAYER_TELEPORTED){
		call(TogedemaruFailed)
	}
    waitmessage
	releaseall
    end
}
 
script TogedemaruWon_Caught {
    fadescreen(FADE_TO_BLACK)
    removeobject(9)
	fadescreenspeed(FADE_FROM_BLACK, 5)
	return
}
 
script TogedemaruFailed {
	playmoncry(SPECIES_TOGEDEMARU, CRY_MODE_ENCOUNTER)
	delay(20)
	fadescreen(FADE_TO_BLACK)
	removeobject(9)
	fadescreenspeed(FADE_FROM_BLACK, 5)
    msgbox("The TOGEDEMARU disappeared…")
	return
}

script NewMauville_Inside_EventScript_Raikou{
    lock
	faceplayer
    playmoncry(SPECIES_RAIKOU, CRY_MODE_ENCOUNTER)
    delay(40)
    waitmoncry
    setwildbattle(SPECIES_RAIKOU, 75)
    special(BattleSetup_StartLegendaryBattle)
    waitstate
    specialvar(VAR_RESULT, GetBattleOutcome)
    if(var(VAR_RESULT) == B_OUTCOME_CAUGHT || var(VAR_RESULT) == B_OUTCOME_WON){
        fadescreen(FADE_TO_BLACK)
        removeobject(VAR_LAST_TALKED)
	    fadescreenspeed(FADE_FROM_BLACK, 5)
	}
	elif(var(VAR_RESULT) == B_OUTCOME_RAN || var(VAR_RESULT) == B_OUTCOME_PLAYER_TELEPORTED){
		goto(Common_EventScript_NopReturn)
    }
	release
	end
}

