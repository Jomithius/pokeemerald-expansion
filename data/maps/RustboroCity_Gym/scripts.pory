script RustboroCity_Gym_EventScript_GiveChillingWater {
	msgbox("And because you were on fire,\n
	take this to cool off!", MSGBOX_AUTOCLOSE)
    waitmessage
    giveitem(ITEM_TM_CHILLING_WATER)
    waitmessage
    msgbox(RustboroCity_Gym_Text_ExplainChillingWater, MSGBOX_AUTOCLOSE)
    waitmessage
    setvar (VAR_MISA_STATE, 3)
	setrespawn(HEAL_LOCATION_RUSTBORO_CITY) // to avoid player missing rat cave trigger by teleporting/whiting out back to petalburg
    release
    end

}

text RustboroCity_Gym_Text_ExplainChillingWater{
    "That TECHNICAL MACHINE, TM52,\n
	contains CHILLING WATER.\l
    It's a good way to put out a POKéMON\l
    that's on fire!"
}

script RustboroCity_Gym_Magmortar{
	lock
	playmoncry(SPECIES_MAGMORTAR, CRY_MODE_ENCOUNTER)
	delay(20)
	release
	end
}